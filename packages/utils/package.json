{
  "name": "@zionstate/utils",
  "version": "0.0.1",
  "private": true,
  "description": "Zion Utils",
  "source": "src/index.ts",
  "main": "dist/zionstate-utils.cjs.js",
  "module": "dist/zionstate-utils.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "preconstruct": {
    "entrypoint": [
      "src/index.ts"
    ]
  },
